{
    "name": "Fakescore",
    "version": "1.0",
    "description": "An extension to help you detect Filipino fake news in the Internet.",
    "manifest_version": 2,
    "permissions": [
        "activeTab", 
        "tabs",
        "http://127.0.0.1:8000/api/"],
    "browser_action": {
        "default_title": "Fakescore",
        "default_popup": "index.html",
        "default_icon": "icon.png"
    }
    
    
  
}
  